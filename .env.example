# Copy to .env and fill in your own values

# LLM Provider - OpenRouter
STORYCRAFT_OPENROUTER_API_KEY=
STORYCRAFT_OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
STORYCRAFT_OPENROUTER_DEFAULT_MODEL=deepseek/deepseek-chat-v3-0324

# Database - Supabase
STORYCRAFT_SUPABASE_URL=
STORYCRAFT_SUPABASE_SERVICE_KEY=
# Optional: Direct PostgreSQL connection for setup scripts
STORYCRAFT_SUPABASE_DB_URL=

# CORS Configuration (optional)
# Comma-separated list of additional allowed origins
# STORYCRAFT_CORS_ORIGINS=
# Optional regex pattern for dynamic origins (e.g., Vercel deployments)
# STORYCRAFT_CORS_ORIGIN_REGEX=
